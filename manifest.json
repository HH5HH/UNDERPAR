{
  "manifest_version": 3,
  "name": "MinCloudLogin",
  "version": "1.0.39",
  "description": "Minimal Adobe Experience Cloud login extension.",
  "minimum_chrome_version": "114.0.0.0",
  "key": "",
  "permissions": [
    "identity",
    "storage",
    "sidePanel",
    "tabs",
    "debugger",
    "webRequest",
    "cookies",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "<all_urls>",
    "https://ims-na1.adobelogin.com/ims/*",
    "https://*.adobelogin.com/*",
    "https://*.adobe.com/*",
    "https://login.aepdebugger.adobe.com/*",
    "https://console.auth.adobe.com/*",
    "https://sp.auth.adobe.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html",
  "action": {
    "default_title": "MinCloudLogin",
    "default_icon": {
      "16": "experience_platform_16.1bcafd66.png",
      "48": "experience_platform_48.041caf2b.png",
      "128": "experience_platform_128.5d253c40.png"
    }
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "icons": {
    "16": "experience_platform_16.1bcafd66.png",
    "48": "experience_platform_48.041caf2b.png",
    "128": "experience_platform_128.5d253c40.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "src/login/login.html"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}
