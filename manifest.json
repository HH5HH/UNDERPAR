{
  "manifest_version": 3,
  "name": "UnderPAR",
  "short_name": "UP",
  "version": "1.0.48",
  "description": "UnderPAR (UP) PASS Acceleration Layer. Keep PASS workflows under par.",
  "minimum_chrome_version": "114.0.0.0",
  "key": "",
  "permissions": [
    "identity",
    "storage",
    "sidePanel",
    "tabs",
    "debugger",
    "webRequest",
    "cookies",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "<all_urls>",
    "https://ims-na1.adobelogin.com/ims/*",
    "https://*.adobelogin.com/*",
    "https://*.adobe.com/*",
    "https://login.aepdebugger.adobe.com/*",
    "https://console.auth.adobe.com/*",
    "https://sp.auth.adobe.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html",
  "action": {
    "default_title": "UP",
    "default_icon": {
      "16": "icons/underpar-16.png",
      "24": "icons/underpar-24.png",
      "32": "icons/underpar-32.png"
    }
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "icons": {
    "16": "icons/underpar-16.png",
    "32": "icons/underpar-32.png",
    "48": "icons/underpar-48.png",
    "128": "icons/underpar-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "src/login/login.html"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "version_name": "1.0.48"
}
